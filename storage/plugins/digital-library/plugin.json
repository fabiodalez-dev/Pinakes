{
  "name": "digital-library",
  "display_name": "Digital Library (eBooks/Audiobooks)",
  "version": "1.0.0",
  "author": "Fabiodalez",
  "author_url": "",
  "plugin_url": "",
  "description": "Gestione eBooks (PDF/ePub) e audiobooks con player integrato Green Audio Player. Consente upload e riproduzione di contenuti digitali direttamente nella biblioteca.",
  "main_file": "wrapper.php",
  "requires_php": "8.0",
  "requires_app": "0.4.0",
  "max_app_version": "1.0.0",
  "metadata": {
    "category": "media",
    "priority": 10,
    "php_extensions": [
      "gd",
      "fileinfo"
    ],
    "assets": {
      "css": [
        "public/assets/vendor/green-audio-player/css/green-audio-player.min.css",
        "storage/plugins/digital-library/assets/css/digital-library.css"
      ],
      "js": [
        "public/assets/vendor/green-audio-player/js/green-audio-player.min.js"
      ]
    },
    "hooks": []
  }
}
